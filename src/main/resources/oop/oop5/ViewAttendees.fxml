<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="oop.oop5.ViewAttendees" alignment="CENTER" hgap="10" vgap="10" prefHeight="550" prefWidth="950" >
    <Label text="View Attendees" GridPane.rowIndex="0" GridPane.columnSpan="2" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <TableView fx:id="attendeeTable" GridPane.rowIndex="1" GridPane.columnSpan="2">
        <columns>
            <TableColumn text="Username" fx:id="attendeeNameColumn"/>
            <TableColumn text="Balance" fx:id="balanceColumn"/>
        </columns>
    </TableView>

    <Button text="Back to Dashboard" fx:id="backButton" onAction="#back" GridPane.rowIndex="2" GridPane.columnSpan="2"/>
</GridPane>
