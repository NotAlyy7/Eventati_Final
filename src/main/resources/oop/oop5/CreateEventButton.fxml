<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="813.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.oop5.CreateEventController">
    <children>
        <VBox alignment="BASELINE_CENTER" prefHeight="551.0" prefWidth="190.0" spacing="65.0" style="-fx-background-color: #1A2B4C;">
            <children>
                <Label text="Dashboard" textFill="#fffdfd">
                    <font>
                        <Font name="System Bold" size="27.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" style="-fx-background-color: #0056b3;" text="Create Event" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#SwitchtoUpdateEvent" style="-fx-background-color: #0056b3;" text="Update Event" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#SwitchtoCancelEvent" style="-fx-background-color: #0056b3;" text="Cancel Event" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#SwitchtoLogout" style="-fx-background-color: #0056b3;" text="Logout" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
            </children>
        </VBox>

        <TextField fx:id="eventIdField" layoutX="312.0" layoutY="54.0" />
        <TextField fx:id="eventNameField" layoutX="597.0" layoutY="54.0" />
        <TextField fx:id="descriptionField" layoutX="311.0" layoutY="136.0" />
        <DatePicker fx:id="datePicker" layoutX="298.0" layoutY="213.0" />
        <ChoiceBox fx:id="timeSlotChoiceBox" layoutX="596.0" layoutY="213.0" prefWidth="150.0" />
        <TextField fx:id="priceField" layoutX="317.0" layoutY="302.0" />
        <ChoiceBox fx:id="roomChoiceBox" layoutX="312.0" layoutY="404.0" prefWidth="150.0" />

        <Button fx:id="createEventButton" layoutX="524.0" layoutY="472.0" mnemonicParsing="false" onAction="#HandleCreateEvent" style="-fx-background-color: #0056b3;" text="Confirm" textFill="WHITE">
            <font>
                <Font name="System Bold Italic" size="18.0" />
            </font>
        </Button>

        <Label layoutX="242.0" layoutY="58.0" text="Event ID:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label layoutX="506.0" layoutY="58.0" text="Event Name:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="247.0" layoutY="217.0" text="Date:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="228.0" layoutY="140.0" text="Description:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="515.0" layoutY="306.0" text="Category:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="228.0" layoutY="306.0" text="Ticket Price:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="514.0" layoutY="217.0" text="Time Slot:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="247.0" layoutY="408.0" text="Room:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="createEventButton1" layoutX="665.0" layoutY="472.0" mnemonicParsing="false" onAction="#SwitchToDashboard" style="-fx-background-color: #0056b3;" text="Return" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="categoryField" layoutX="596.0" layoutY="302.0" prefWidth="150.0" />
    </children>
</AnchorPane>
