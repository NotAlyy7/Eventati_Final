<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="503.0" prefWidth="813.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.oop5.HelloController">
    <children>
        <VBox alignment="BASELINE_CENTER" prefHeight="551.0" prefWidth="190.0" spacing="65.0" style="-fx-background-color: #1A2B4C;">
            <children>
                <Label text="Dashboard" textFill="#fffdfd">
                    <font>
                        <Font name="System Bold" size="27.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" onAction="#SwitchtoCreateEvent" style="-fx-background-color: #0056b3;" text="Create Event" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#SwitchtoUpdateEvent" style="-fx-background-color: #0056b3;" text="Update Event" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#SwitchtoCancelEvent" style="-fx-background-color: #0056b3;" text="Cancel Event" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#SwitchtoLogout" prefHeight="40.0" prefWidth="132.0" style="-fx-background-color: #0056b3;" text="Logout" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Button>
            </children>
        </VBox>
        <Label layoutX="347.0" layoutY="185.0" prefHeight="90.0" prefWidth="295.0" text="Welcome, Organizer!">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label layoutX="234.0" layoutY="262.0" prefHeight="90.0" prefWidth="578.0" text="Use the menu on the left to create or manage your events.">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
      <TextField fx:id="balancefield" editable="false" layoutX="388.0" layoutY="409.0">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </TextField>
    </children>
</AnchorPane>
