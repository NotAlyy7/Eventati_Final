<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="813.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.oop5.CancelEventController">
   <children>
      <VBox alignment="BASELINE_CENTER" prefHeight="551.0" prefWidth="190.0" spacing="65.0" style="-fx-background-color: #1A2B4C;">
         <children>
            <Label text="Dashboard" textFill="#fffdfd">
               <font>
                  <Font name="System Bold" size="27.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#SwitchtoCreateEvent" style="-fx-background-color: #0056b3;" text="Create Event" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#SwitchtoUpdateEvent" style="-fx-background-color: #0056b3;" text="Update Event" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Button>
            <Button defaultButton="true" mnemonicParsing="false" style="-fx-background-color: #0056b3;" text="Cancel Event" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#SwitchtoLogout" style="-fx-background-color: #0056b3;" text="Logout" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <ChoiceBox fx:id="eventChoiceBox" layoutX="486.0" layoutY="263.0" prefWidth="150.0" />
      <Button layoutX="527.0" layoutY="472.0" mnemonicParsing="false" style="-fx-background-color: #0056b3;" text="Confirm" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <Label layoutX="300.0" layoutY="267.0" text="Choose Event to be Canceled:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button layoutX="671.0" layoutY="472.0" mnemonicParsing="false" onAction="#SwitchToDashboard" style="-fx-background-color: #0056b3;" text="Return" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
